{
    "manifest_version": 3,
    "name": "Meta Extractor",
    "version": "1.0",
    "action": {
      "default_popup": "index.html"
    },
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "web_accessible_resources": [{
      "resources": ["static/js/main.js"],
      "matches": ["<all_urls>"]
    }]
}